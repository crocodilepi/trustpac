parameters
{
   title "PEMFC cas test dissous de Hydrogen dans Nafion (MB+CLa)"
   description "Ce cas test a pour l'objectif de tester le modele de dissolution de gaz Hydrogen dans Nafion"
   description "Hypothese: les champs couplage sont constants: temperature T, concentration Hydrogen"
   description "Dimension "
   description "Largeur 20e-6 m"
   description "Hauteur MB: 25e-6m"
   description "Hauteur CLa=CLc: 10e-6m"
   description "Hauteur MPLa=MPLc: 40e-6m"
   description "Hauteur GDLa=GDLc: 200e-6 m"
   author "Van quang DINH"
   testcase .    pb_Ch.data
   testcase . PAR_pb_Ch.data 2   
}

chapter
{
   title "Resultat pb_Ch"
   description "Profil de concentration dissous Ch (autrement note C_H2) "
   
   Visu {
	Title "Champ de concentration dissous Ch aux sommets [mol/m3] "
	pseudocolor pb_Ch.lata dom_C_H2 TEMPERATURE SOM
	cycles -1
	width 15cm
   }
   
   Figure {
	Title "Champ de concentration dissous Ch coupe au plan milieu [mol/m3]"
	Dimension 2
	LabelX x
	LabelY V
        include_description_curves 0
	width 12cm
	
	Curve {
		legend pb_Ch
		segment pb_Ch CH_SEG
		Style lines
    }	
    Curve {
		legend PAR_pb_Ch
		segment PAR_pb_Ch CH_SEG
		Style lines
    }	
   }

   
   Visu {
	Title "Chefficient de diffusion effective 'diffusion_nafion' Da_H2_eff"
	pseudocolor pb_Ch.lata dom_C_H2 DIFFUSION_NAFION ELEM
	cycles -1
	width 15cm
   }
   
   Visu {
	Title "Chefficient de diffusion normal 'Da_H2_naf'"
	pseudocolor pb_Ch.lata dom_C_H2 D_I_NAF ELEM
	cycles -1
	width 15cm
   }
   
   Visu {
	Title "Approximation de Terme Source aux centres d'element Sa_H2"
	pseudocolor pb_Ch.lata dom_C_H2 S ELEM
	cycles -1
	width 15cm
   }
   
   Visu {
	Title "Approximation de concentration dissouse equivalent Ceq aux centres d'element"
	pseudocolor pb_Ch.lata dom_C_H2 CEQ ELEM
	cycles -1
	width 15cm
   }
   
   Visu {
	Title "Terme Source de diffusion Sa_H2 aux centres d'element"
	pseudocolor pb_Ch.lata dom_C_H2 SA_H2 ELEM
	cycles -1
	width 15cm
   }
   
   Visu {
	Title "Terme Source de reaction S_H2 aux centres d'element"
	pseudocolor pb_Ch.lata dom_C_H2 S_H2 ELEM
	cycles -1
	width 15cm
   }
   
   Figure {
	Title "convergence du solveur"
	Dimension 2
	LabelX t
	LabelY res
	logy
        include_description_curves 0
	width 12cm
	Curve {
		legend pb_Ch
		file pb_Ch.dt_ev
		columns $1 $4
		Style lines
   }
   Curve {
		legend PAR_pb_Ch
		file PAR_pb_Ch.dt_ev
		columns $1 $4
		Style lines
   }
  }
  
}
