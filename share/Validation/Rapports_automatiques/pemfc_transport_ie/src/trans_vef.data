# Thermohydraulique 2D couplee a conduction 2D #
# PARALLEL NOT #
dimension 2

# BEGIN MESH #
Domaine dom

Mailler dom
{
	Pave Cavite1
	{
	    Origine 0. 0. 
	    Nombre_de_Noeuds 2 26	
	    Longueurs 1.0 25.0
	}
	{
	    Bord Gauche    X = 0.   0.  <= Y <= 25.0
	    Bord Droit     X = 1.   0.  <= Y <= 25.0
	}
,
  	Pave Cavite2
	{
	    Origine 0. 25.
        Nombre_de_Noeuds 2 11
     	Longueurs 1.0 10.0
	}
	{
	    Bord Gauche      X = 0.   25. <= Y <= 35.0	
	    Bord Droit       X = 1.   25. <= Y <= 35.0
	}
, 
	Pave Cavite3
	{
		Origine 0. 35.
        Nombre_de_Noeuds 2 41
		Longueurs 1.0 40.0
	}
	{
		Bord Gauche       X = 0.   35.  <= Y <= 75.0
		Bord Droit        X = 1.   35. <= Y <= 75.0
	}
, 
	Pave Cavite4
	{
		Origine 0. 75.
        Nombre_de_Noeuds 2 201
		Longueurs 1.0 200.0
	}
	{
		Bord Gauche X = 0.    75.  <= Y <= 275.0
		Bord Droit  X = 1.    75.  <= Y <= 275.0
		Bord Haut   Y = 275.0  0.  <= X <= 1.0
	}
,
  	Pave Cavite5
	{
	    Origine 0. -10.
        Nombre_de_Noeuds 2 11
     	Longueurs 1.0 10.0
	}
	{
	    Bord Gauche      X = 0.   -10.  <= Y <= 0.0	
	    Bord Droit       X = 1.   -10.  <= Y <= 0.0
	}
,
	Pave Cavite6
	{
		Origine 0. -50.
        Nombre_de_Noeuds 2 41
		Longueurs 1.0 40.0
	}
	{
		Bord Gauche       X = 0.    -50.  <= Y <= -10.0
		Bord Droit        X = 1.    -50.  <= Y <= -10.0
	}
, 
	Pave Cavite7
	{
		Origine 0. -250.
        Nombre_de_Noeuds 2 201
		Longueurs 1.0 200.0
	}
	{
		Bord Gauche       X = 0.     -250.  <= Y <= -50.0
		Bord Droit        X = 1.     -250.  <= Y <= -50.0
		Bord Bas          Y = -250.0    0.  <= X <= 1.0
	}
}
Transformer dom x*1e-6 y*1e-6
trianguler_h dom

Sous_zone elec
associer elec dom
Lire elec { 
  fonction_sous_zone (y_GE_(25.e-6)_OR_(y_LE_(0.0)))
}

Sous_zone ion 
associer ion dom
Lire ion { 
  fonction_sous_zone (y_LE_(35.e-6)_AND_(y_GE_(-10.0e-6)))
}

Domaine dom_electrique
Create_domain_from_sous_zone { domaine_final dom_electrique domaine_init dom par_sous_zone elec }

Domaine dom_ionique
Create_domain_from_sous_zone { domaine_final dom_ionique domaine_init dom par_sous_zone ion }

Sous_zone GDL 
associer GDL dom_electrique
Lire GDL { fonction_sous_zone (y_GE_(75.e-6)_OR_(y_LE_(-50.e-6))) }

Sous_zone MPL 
associer MPL dom_electrique
Lire MPL { fonction_sous_zone ((y_GE_(35.e-6))_AND_(y_LE_(75.e-6)))_OR_((y_GE_(-50.e-6))_AND_(y_LE_(-10.e-6))) }

Sous_zone CL_electrique 
associer CL_electrique dom_electrique
Lire CL_electrique { fonction_sous_zone ((y_GE_(25.e-6))_AND_(y_LE_(35.e-6)))_OR_((y_GE_(-10.e-6))_AND_(y_LE_(0.0))) }

Sous_zone CL_ionique 
associer CL_ionique dom_ionique
Lire CL_ionique { fonction_sous_zone ((y_GE_(25.e-6))_AND_(y_LE_(35.e-6)))_OR_((y_GE_(-10.e-6))_AND_(y_LE_(0.0))) }

Sous_zone MB 
associer MB dom_ionique
Lire MB { fonction_sous_zone (y_GE_(0.0)_OR_(y_LE_(25.e-6))) }

postraiter_domaine { domaines { dom dom_electrique dom_ionique } format lata }
# END MESH #

