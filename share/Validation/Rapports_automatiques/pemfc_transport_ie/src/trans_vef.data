# END MESH #

# BEGIN PARTITION
Partition dom_ionique
{
	Partition_tool tranche { tranches 3 1 }
	Larg_joint 1
	zones_name DOM1
}
Partition dom_electrique
{
	Partition_tool tranche { tranches 3 1 }
	Larg_joint 1
	zones_name DOM2
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM1.Zones dom_ionique
Scatter DOM2.Zones dom_electrique
END SCATTER #

Scheme_euler_implicit sch
Read sch
{
	tinit 0.
	tmax 1.
	dt_min 1.e-20
	dt_max 1.
	dt_impr 0.001
	dt_sauv 400.
	seuil_statio 1.e-2
    solveur implicite { solveur  gmres { diag seuil -1 nb_it_max 5 } seuil_convergence_implicite 100. }
    facsec 1e4
    facsec_max 1e4
}

Pb_conduction pb_electrique
Pb_conduction pb_ionique

VEFPrep1B dis

Solide prop_electrique
Read prop_electrique
{
	rho Champ_Uniforme 1 1.
	Cp Champ_Uniforme 1 1.
	lambda champ_uniforme_morceaux dom_electrique 1 { defaut 65.  CL_electrique 20. MPL 10. }
}

Solide prop_ionique
Read prop_ionique
{
	rho Champ_Uniforme 1 1.
	Cp Champ_Uniforme 1 1.
	lambda champ_uniforme_morceaux dom_ionique 1 { defaut 12.  CL_ionique 1.5  }
}

Associate pb_ionique dom_ionique
Associate pb_ionique prop_ionique

Associate pb_electrique dom_electrique
Associate pb_electrique prop_electrique

Probleme_Couple pbc
Associate pbc pb_ionique
Associate pbc pb_electrique
Associate pbc sch
Discretize pbc dis

Read pb_ionique
{
	Conduction
	{
		diffusion { }
		# the ionic potential treated such as the temperature #
		initial_conditions { temperature Champ_Uniforme 1 0. }
		
		boundary_conditions {
			Gauche     paroi_adiabatique
			Droit      paroi_adiabatique
			Couture_0 paroi_adiabatique
		}
	
		sources { 
		    puissance_thermique Champ_couplage_transport_ie 
		    {
		        nom_pb_electrique pb_electrique 
		        nom_dom_electrique dom_electrique 
		        nom_szz_electrique CL_electrique 
		        nom_pb_ionique pb_ionique 
		        nom_dom_ionique dom_ionique 
		        nom_szz_ionique CL_ionique 
		        nom_pb pb_ionique
		    }
	    }
    }	
	Post_processing
	{
	    Definition_champs 
	    {
	        psi_inter interpolation { localisation elem domaine dom_ionique sources { refchamp { pb_champ pb_electrique temperature } } }
	        diff operateur_eqn { numero_op 0 source refchamp {  pb_champ pb_ionique temperature } }
		    s operateur_eqn { numero_source 0 source refchamp {  pb_champ pb_ionique temperature } }
        }	
	    Probes
	    {
	        T temperature periode 1e-20 points 1 0.5e-6 30e-6
		    P puissance_volumique periode 1e-20 points 1 0.5e-6 30e-6	
            phi grav temperature periode 1 segment 100  0.5e-6  -10e-6 0.5e-6 35e-6 # grav = value to centre of the nearest cell #
            Pseg grav  puissance_volumique  periode 1 segment 100  0.5e-6  25e-6 0.5e-6 35e-6
        }
		Format lata
		fields dt_post 10.
		{
	        psi_inter
		    TEMPERATURE ELEM	# ionic potential #
		    puissance_volumique elem	
		    S 
		    diff
	        conductivite elem
		}
	}
}


Read pb_electrique
{
	
	Conduction {
		diffusion { }
		initial_conditions {
			temperature Champ_Uniforme 1 0.7
		}
		boundary_conditions {
			Gauche    paroi_adiabatique
			Droit     paroi_adiabatique
			Haut      paroi_temperature_imposee Champ_Front_Uniforme 1 0.7
			Bas       paroi_temperature_imposee Champ_Front_Uniforme 1 0.
		 	Couture_0 paroi_adiabatique
		}
	    sources { 
	        puissance_thermique Champ_couplage_transport_ie 
	        {
		        nom_pb_electrique pb_electrique 
		        nom_dom_electrique dom_electrique 
		        nom_szz_electrique CL_electrique 
		        nom_pb_ionique pb_ionique 
		        nom_dom_ionique dom_ionique 
		        nom_szz_ionique CL_ionique 
		        nom_pb pb_electrique
		    }
	    }
	}

	Post_processing
	{
	    Definition_champs 
	    {
	        phi_inter interpolation { localisation elem domaine dom_electrique sources  {  refchamp  { pb_champ pb_ionique temperature } } } 
	    }
	    Probes
	    {
	        T2 temperature periode 1e-20 points 1 0.5e-6 30e-6
            PsiC grav temperature periode 1 segment 250  0.5e-6  25e-6 0.5e-6 275e-6
            PsiA grav temperature periode 1 segment 250  0.5e-6  -250e-6 0.5e-6 0.
	    }
		Format lata
		fields dt_post 10.
		{
		    TEMPERATURE ELEM		# electric potential #
		    puissance_volumique elem
		    conductivite elem
		}
	}
}

Solve pbc

End

