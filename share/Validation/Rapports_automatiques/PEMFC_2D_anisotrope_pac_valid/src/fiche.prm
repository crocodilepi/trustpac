chapter
{
   title "Thermal diffusivity"
   description "Here is the visualization of all components of the thermal diffusivity located at elements."
   Visu {
	Title "component 0"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom DIFFUSIVITE_THERMIQUE_0 ELEM
	cycles -1
	width 15cm
    }
   Visu {
	Title "component 1"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom DIFFUSIVITE_THERMIQUE_1 ELEM
	cycles -1
	width 15cm
    }
   Visu {
	Title "component 2"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom DIFFUSIVITE_THERMIQUE_2 ELEM
	cycles -1
	width 15cm
    }
   Visu {
	Title "component 3"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom DIFFUSIVITE_THERMIQUE_3 ELEM
	cycles -1
	width 15cm
    }
    
    description "Here can be added some comments"
}

chapter
{
   title "Temperature"
   description "Here is the visualization of the temperature."
      Visu {
	Title "Temperature field located at vertices"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom TEMPERATURE SOM
	cycles -1
	width 15cm
    }
}

chapter {
   title "Gradient of Temperature"
   description "Gradient of Temperature"
   Visu {
	Title "Flux field located at vertices"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom GRADT_X SOM
	cycles -1
	width 15cm
    }
    Visu {
	Title "Flux field located at vertices"
	pseudocolor PEMFC_2D_anisotrope_pac.lata dom GRADT_Y SOM
	cycles -1
	width 15cm
    }
}


chapter
{
   title "Comparison of temperature between Comsol and TRUST on the segment [P1,P2] with P1=(0;1) and P2=(10;0) "
   figure
   {
      dimension 2
      include_description_curves 0
      labelx "Position"
      labely "Temperature"
      width  12cm
      curve
      {
        legend "TRUST"
	file trust_temperature_sur_diagonal.txt
	columns $1 $2
	style linespoints
      }
      curve
      {
        legend "Comsol"
	file comsol_temperature_sur_diagnonal.txt
	columns $1 $2
	style linespoints
      }
   }
}

chapter
{
   title "Comparison of flux between Comsol and TRUST  on boundary Haut"
   figure {
      title "Flux au bord Haut"
      dimension 2
      include_description_curves 0
      labelx "x"
      labely "flux"
      width  12cm
      curve
      {
        legend "TRUST"
	file trust_flux_bord_haut.txt
	columns $1 $4
	style linespoints
      }
      curve
      {
        legend "Comsol"
	file comsol_flux_sur_surface_bord_haut.txt
	columns $1 $2
	style linespoints
      }
   }
}

